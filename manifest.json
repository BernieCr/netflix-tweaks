{
  "name": "Netflix Tweaks",
  "version": "0.2",
  "manifest_version": 2,
  "permissions": [
    "https://www.netflix.com/*",
    "storage"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}