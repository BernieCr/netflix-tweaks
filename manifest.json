{
  "name": "Netflix Tweaks",
  "version": "0.3",
  "manifest_version": 2,
  "permissions": [
    "https://www.netflix.com/*",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}